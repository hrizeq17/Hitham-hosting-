<%= form_with(model: @student, local: true) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :semester_id, "Semester" %>
  <%= form.collection_select :semester_id, Semester.all, :id, :name_for_select, include_blank: true %>

  <%= form.submit "Add Student" %>
<% end %>
